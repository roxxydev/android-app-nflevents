<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/frag_menu_list_item_container"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="@dimen/dimen_frag_venue_list_item_margin">

    <TextView
        android:id="@+id/frag_venue_list_item_tv_name"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimen_frag_venue_list_item_tv_margin_leftright"
        android:layout_marginRight="@dimen/dimen_frag_venue_list_item_tv_margin_leftright"
        android:layout_marginTop="@dimen/dimen_frag_venue_list_item_tv_margin_topbottom"
        android:layout_marginBottom="@dimen/dimen_frag_venue_list_item_tv_margin_topbottom"
        android:textSize="@dimen/dimen_frag_venue_list_item_name_tv_size"/>

    <TextView
        android:id="@+id/frag_venue_list_item_tv_address"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimen_frag_venue_list_item_tv_margin_leftright"
        android:layout_marginRight="@dimen/dimen_frag_venue_list_item_tv_margin_leftright"
        android:layout_marginTop="@dimen/dimen_frag_venue_list_item_tv_margin_topbottom"
        android:layout_marginBottom="@dimen/dimen_frag_venue_list_item_tv_margin_topbottom"
        android:textSize="@dimen/dimen_frag_venue_list_item_address_tv_size"/>

</LinearLayout>